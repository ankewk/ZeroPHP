{
    "name": "ankewk/zerophp",
    "description": "mini php framework",
    "license": "MIT",
    "authors": [
        {
            "name": "ankewk",
            "email": "171640567@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
    	"psr-4": {
            "": "app",
    		"Zero\\": "zero"
    	}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
